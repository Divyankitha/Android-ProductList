<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.divyankitharaghavaurs.product">

    <application
        android:allowBackup="true"
        android:icon="@drawable/sjsu_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".insertActivity" />
        <activity android:name=".searchActivity" />
        <activity android:name=".productNotFoundDialog"
            android:theme="@style/Base.Theme.AppCompat.Light.Dialog.FixedSize"/>
        <activity android:name=".ProductAddDialog"
            android:theme="@style/Base.Theme.AppCompat.Light.Dialog.FixedSize"/>
    </application>

</manifest>